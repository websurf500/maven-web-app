node {
    def mvnPath
    
    stage('git clone') {
        git 'https://github.com/suffixscope/maven-web-app.git'
    }
    
    stage('Maven build'){
        def mvnHome = tool name:'maven-3.9.9', type:"maven";
        mvnPath = "${mvnHome}/bin/mvn";
        sh "${mvnPath} clean package"
    }
}

